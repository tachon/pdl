(VAR ss1 ss2 ss t p)
(RULES
  Rput(ConsSS(ss1,ss2)) -> ConsVS(RunmatchSec(ss1),Rput(ss2))
  Rput(empty) -> empty
  RunmatchSec(ss) -> RmatchSec(ss)
  RunmatchSec(SecS(t, newSec, ss)) -> SecV(t, RallNewSub(ss))
  RmatchSec(SecS(t, p, ss)) -> SecV(t, RputSub(ss))
  RallNewSub(ConsSB(SubS(t,newSub),ss)) -> ConsVB(t,RallNewSub(ss))
  RallNewSub(empty) -> empty
  RputSub(ConsSB(ss1,ss2)) -> ConsVB(RunmatchSub(ss1),RputSub(ss2))
  RputSub(empty) -> empty
  RunmatchSub(ss) -> Rmatch(ss)
  RunmatchSub(SubS(t,newSub)) -> t
  Rmatch(SubS(t,p)) -> t
)